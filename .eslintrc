{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/jsx-indent": 0,
    "indent": [1, 4, { "SwitchCase": 1 }],
    "react/jsx-indent-props": [1, 4],
    "react/destructuring-assignment": 0,
    "react/require-default-props": 0,
    "react/forbid-prop-types": 0
  }
}